{
  "name": "viper/installer",
  "description": "Viper application installer.",
  "keywords": [ "viper", "framework", "installer" ],
  "license": "MIT",
  "authors": [
    {
      "name": "Michael NoÃ«l",
      "email": "mike@viperframe.work"
    }
  ],
  "require": {
    "php": "^7.2.9",
    "ext-zip": "*",
    "guzzlehttp/guzzle": "^6.0",
    "symfony/console": "^4.0|^5.0",
    "symfony/filesystem": "^4.0|^5.0",
    "symfony/process": "^4.2|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "bin": [
    "bin/viper"
  ],
  "autoload": {
    "psr-4": {
      "Viper\\Installer\\Console\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Viper\\Installer\\Console\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
